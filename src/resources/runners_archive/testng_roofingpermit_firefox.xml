<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="HouPerProject suite" verbose="2">
	<!--parallel="tests" thread-count="3"-->
	<listeners>
		<listener class-name="com.qa.houperpro.listeners.ExtentReportListener" />
		<listener class-name="com.qa.houperpro.listeners.ScreenshotListener" />
	</listeners>

	<test name="RoofingPermit Page test">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.qa.houperpro.tests.RoofingPermitTest" />
		</classes>
	</test>
	<test name="Login Page test">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.qa.houperpro.tests.LoginPage_HouPerTest" />
		</classes>
	</test>
</suite>